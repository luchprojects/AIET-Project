{
  "version": "4.0",
  "description": "NASA-locked feature contract for AIET ML habitability model",
  "created": "2026-01-26",
  "model_type": "XGBoost Regressor",
  "target": "Relative habitability score (0-1 scale, NOT probability of life)",
  
  "features": [
    {
      "name": "pl_rade",
      "index": 0,
      "description": "Planet radius",
      "units": "Earth radii (R⊕)",
      "source": "NASA exoplanets.csv",
      "imputation": "median (6.0 R⊕)",
      "range": [0.1, 20.0]
    },
    {
      "name": "pl_masse",
      "index": 1,
      "description": "Planet mass",
      "units": "Earth masses (M⊕)",
      "source": "NASA exoplanets.csv",
      "imputation": "If pl_rade < 1.6: pl_rade^2.06 (rocky M-R relation), else: median by radius bin",
      "range": [0.001, 500.0]
    },
    {
      "name": "pl_orbper",
      "index": 2,
      "description": "Orbital period",
      "units": "days",
      "source": "NASA exoplanets.csv",
      "imputation": "median (10.0 days)",
      "range": [0.1, 100000.0]
    },
    {
      "name": "pl_orbsmax",
      "index": 3,
      "description": "Semi-major axis (orbital distance)",
      "units": "AU",
      "source": "NASA exoplanets.csv",
      "imputation": "Compute from Kepler's 3rd law: (P_yr^2 * st_mass)^(1/3), where P_yr = pl_orbper/365.25",
      "range": [0.01, 1000.0]
    },
    {
      "name": "pl_orbeccen",
      "index": 4,
      "description": "Orbital eccentricity",
      "units": "unitless (0=circular, <1=ellipse)",
      "source": "NASA exoplanets.csv",
      "imputation": "0.0 (assume circular)",
      "range": [0.0, 1.0]
    },
    {
      "name": "pl_insol",
      "index": 5,
      "description": "Insolation flux received by planet",
      "units": "Earth flux (S⊕)",
      "source": "NASA exoplanets.csv",
      "imputation": "st_lum / (pl_orbsmax^2) if both available",
      "range": [0.0001, 100.0]
    },
    {
      "name": "pl_eqt",
      "index": 6,
      "description": "Equilibrium temperature (no atmosphere)",
      "units": "Kelvin (K)",
      "source": "NASA exoplanets.csv",
      "imputation": "278.5 * (pl_insol^0.25) if pl_insol available (Earth albedo proxy)",
      "range": [50.0, 3000.0]
    },
    {
      "name": "pl_dens",
      "index": 7,
      "description": "Planet bulk density",
      "units": "g/cm³",
      "source": "NASA exoplanets.csv",
      "imputation": "Compute from pl_masse and pl_rade with correct SI->CGS conversion",
      "range": [0.1, 30.0]
    },
    {
      "name": "st_teff",
      "index": 8,
      "description": "Stellar effective temperature",
      "units": "Kelvin (K)",
      "source": "NASA stellar_hosts.csv",
      "imputation": "median by spectral type (G: 5778K)",
      "range": [2000.0, 50000.0]
    },
    {
      "name": "st_mass",
      "index": 9,
      "description": "Stellar mass",
      "units": "Solar masses (M☉)",
      "source": "NASA stellar_hosts.csv",
      "imputation": "median by spectral type (G: 1.0 M☉)",
      "range": [0.08, 100.0]
    },
    {
      "name": "st_rad",
      "index": 10,
      "description": "Stellar radius",
      "units": "Solar radii (R☉)",
      "source": "NASA stellar_hosts.csv",
      "imputation": "median by spectral type (G: 1.0 R☉)",
      "range": [0.1, 1000.0]
    },
    {
      "name": "st_lum",
      "index": 11,
      "description": "Stellar luminosity",
      "units": "Solar luminosities (L☉)",
      "source": "NASA stellar_hosts.csv",
      "imputation": "Stefan-Boltzmann: st_rad^2 * (st_teff/5778)^4",
      "range": [0.0001, 1000000.0]
    }
  ],
  
  "explicitly_excluded": [
    {
      "name": "T_surf",
      "reason": "Too heuristic - not in NASA data, creates training/inference mismatch"
    },
    {
      "name": "greenhouse_offset",
      "reason": "Derived UI parameter, not an ML feature"
    },
    {
      "name": "v_esc_kms",
      "reason": "Perfectly correlated with pl_masse and pl_rade (redundant)"
    },
    {
      "name": "tidal_lock_score",
      "reason": "Perfectly correlated with pl_orbper (redundant)"
    }
  ],
  
  "optional_future_additions": [
    {
      "name": "cb_flag",
      "description": "Circumbinary flag",
      "units": "0/1",
      "justification": "May affect orbital stability"
    },
    {
      "name": "sy_snum",
      "description": "Number of stars in system",
      "units": "count",
      "justification": "Multi-star systems may be less stable"
    },
    {
      "name": "st_met",
      "description": "Stellar metallicity",
      "units": "[Fe/H] dex",
      "justification": "Affects planet formation and composition"
    }
  ],
  
  "imputation_priority": [
    "1. Use NASA value if present",
    "2. Compute from other NASA columns using physics (Kepler, Stefan-Boltzmann, M-R relation)",
    "3. Fill with spectral-type-specific median",
    "4. Last resort: global median"
  ],
  
  "notes": [
    "All features are NASA-sourced or physically derived from NASA columns",
    "No synthetic 'surface temperature' with greenhouse heuristics",
    "No redundant derived features (escape velocity, tidal locking score)",
    "Imputation is deterministic and logged in meta dict",
    "Model outputs 0-1 relative score, NOT probability of life",
    "Earth normalization (=100) is a UI display option only, not in model"
  ]
}
